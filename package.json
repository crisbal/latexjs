{
  "name": "latexjs",
  "version": "0.2.0",
  "description": "A Node.js module for building LaTeX files",
  "keywords": [
    "latex",
    "tex"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/ -d dist/ --source-maps",
    "watch": "babel-watch src/ -d dist/ --source-maps",
    "test": "mocha --require babel-core/register"
  },
  "author": "Cristian Baldi",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-google": "^0.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "escape-latex": "^0.1.3"
  }
}
